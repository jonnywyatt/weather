{
  "name": "wipro",
  "version": "1.0.0",
  "main": "./src/app.js",
  "engines": {
    "node": "6.2.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "node src/app.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha -R spec -b ./test/unit/*-spec.js",
    "test:integration": "mocha -R spec -b ./test/integration/*-spec.js",
    "build": "./node_modules/gulp/bin/gulp.js --gulpfile build/gulpfile.js build",
    "watch": "./node_modules/gulp/bin/gulp.js --gulpfile build/gulpfile.js watch"
  },
  "dependencies": {
    "express": "~4.13.4",
    "hbs": "~4.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "request": "^2.72.0",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0"
  }
}
